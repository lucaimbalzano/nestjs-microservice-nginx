#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lucaimbalzano/Documents/Workspaces/Private/nestjs-microservice/swagger-aggregator/node_modules/.pnpm/swagger-combine@1.4.0_openapi-types@12.1.3/node_modules/swagger-combine/bin/node_modules:/Users/lucaimbalzano/Documents/Workspaces/Private/nestjs-microservice/swagger-aggregator/node_modules/.pnpm/swagger-combine@1.4.0_openapi-types@12.1.3/node_modules/swagger-combine/node_modules:/Users/lucaimbalzano/Documents/Workspaces/Private/nestjs-microservice/swagger-aggregator/node_modules/.pnpm/swagger-combine@1.4.0_openapi-types@12.1.3/node_modules:/Users/lucaimbalzano/Documents/Workspaces/Private/nestjs-microservice/swagger-aggregator/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lucaimbalzano/Documents/Workspaces/Private/nestjs-microservice/swagger-aggregator/node_modules/.pnpm/swagger-combine@1.4.0_openapi-types@12.1.3/node_modules/swagger-combine/bin/node_modules:/Users/lucaimbalzano/Documents/Workspaces/Private/nestjs-microservice/swagger-aggregator/node_modules/.pnpm/swagger-combine@1.4.0_openapi-types@12.1.3/node_modules/swagger-combine/node_modules:/Users/lucaimbalzano/Documents/Workspaces/Private/nestjs-microservice/swagger-aggregator/node_modules/.pnpm/swagger-combine@1.4.0_openapi-types@12.1.3/node_modules:/Users/lucaimbalzano/Documents/Workspaces/Private/nestjs-microservice/swagger-aggregator/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/swagger-combine" "$@"
else
  exec node  "$basedir/../../bin/swagger-combine" "$@"
fi
